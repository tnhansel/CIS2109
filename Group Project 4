/*This is the menu table*/
CREATE TABLE MENU(MenuID integer, 
MenuName varchar(255), 
MenuType varchar(255)
CONSTRAINT employee pk_menu
FOREIGN KEY (EmployeeID)
references EMPLOYEE(EmployeeID));
VALUES (3001, 'Cobb Salad' , 'Salad');

