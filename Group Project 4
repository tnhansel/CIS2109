/*This is the beverage table*/
CREATE TABLE BEVERAGES(BeverageID integer,
BeverageName varchar(255),
BeverageType varchar(255)
CONSTRAINT employee pk_beverage
FOREIGN KEY (EmployeeID)
references EMPLOYEE(EmployeeID));
INSERT INTO BEVERAGES(BeverageID, BeverageName, BeverageType)
